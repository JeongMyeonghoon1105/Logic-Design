/**
 * Not gate:
 * out = not in
 */

CHIP Not {
    IN in;
    OUT out;

    PARTS:
    // Put your code here:
    Nor(a=in, b=in, out=out);
}
/*
Truth Table of NOT(1 Column added for input):
a   a   out
1   1   0
0   0   1

Truth Table of NOR:
a   b   out
1   1   0
1   0   0
0   1   0
0   0   1
*/